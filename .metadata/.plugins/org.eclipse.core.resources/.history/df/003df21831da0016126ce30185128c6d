package rensu;

class data{
  int x;
  // 클래스 멤버 메서드 
     static void change(data d){
		  d. x=1000;
		   System.out.println("바꾸는 중.."+d.x);
	   }
}
public class dekiru {
  public static void main(String args[]){
	  
	  data d=new data();
	  d.x= 10;
	  d. change(d);
	   System.out.printf("After change( )"+d.x);
  }

  }

